


colors_path=$HOME/.cache/wal/colors.sh

cpu_load=$(grep -o "^[^ ]*" /proc/loadavg)

if [ -f "$colors_path" ]; then
  source $colors_path
  echo "^c$color0^^b$color8^ Cpu: ^d^^c$color0^^b$color4^ $cpu_load ^d^"
else 
  printf "^c#3b414d^ ^b#7ec7a2^ CPU"
  printf "^c#abb2bf^ ^b#353b45^ $cpu_load"
fi
